[user]
    name = Anatoliy Ostrovskiy
    email = megatolya@yandex-team.ru
[color]
    ui = true
[color "branch"]
    current = yellow reverse
    local = yellow
    remote = green
[core]
    editor = e
[alias]
    co = checkout
    ci = commit
    st = status
    br = branch
    hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
    type = cat-file -t
    dump = cat-file -p
